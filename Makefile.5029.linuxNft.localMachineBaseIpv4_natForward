all:

#$(NFTbin)     
# NFtext_blockIpv6
# Makefile.5000
# Makefile.5001
# Makefile.5011
# Makefile.5013
# Makefile.5015

# NFTdebug:=

NFTnowTable:=tbLocalIpv4natForward

my_tbLocalIpv4_N9FchainALL :=   \
_i1_N5FcheckSrcIp               \
_i3_N5FcheckDstIp               \
_i5_N5FcheckProtocol_tcp        \
_i7_N5FcheckTcpDstPortNew       \
_j1_N5FcheckProtocol_udp        \
_j3_N5FcheckUdpDstPort          \
_j5_N5FcheckProtocol_icmp       \
_j7_N5FcheckProtocol_igmp       \
_j9_N5Funknown                  \

define NFTtext_29_ipv4_natForward

# NFTtext_29_ipv4_natForward begin
add table ip $(NFTnowTable)         


# add my_tbLocalIpv4_N9FchainALL : begin
$(foreach aa1,$(NFTgroupCard_x_ALL),\
  $(foreach aa2,$(my_tbLocalIpv4_N9FchainALL),\
    $(EOL)add chain ip $(NFTnowTable) chLocalIp4Card_$(aa1)$(aa2)\
  )\
)

# add my_tbLocalIpv4_N9FchainALL : end

add chain ip $(NFTnowTable) chLocalIp4natForWard   { type nat hook forward  priority 100 ; policy accept ; }


$(if 1,
  $(call ip4inRule111,chLocalIp4natForWard, ip protocol == icmp ip daddr 114.114.114.114 $(NFTcnt)  $(NFTset_1), testICMP31) 
  $(call ip4inRule111,chLocalIp4natForWard, ip protocol == icmp ip daddr 8.8.8.8         $(NFTcnt)  $(NFTset_1), testICMP32) 
  $(call ip4inRule111,chLocalIp4natForWard, ip daddr 114.114.114.114 udp dport 53 $(NFTcnt)  $(NFTset_1), testICMP31) 
  $(call ip4inRule111,chLocalIp4natForWard, ip daddr 8.8.8.8         udp dport 53 $(NFTcnt)  $(NFTset_1), testICMP32) 
  $(call ip4inRule111,chLocalIp4natForWard, ip protocol == icmp ip saddr 114.114.114.114 $(NFTcnt)  $(NFTset_1), testICMP31) 
  $(call ip4inRule111,chLocalIp4natForWard, ip protocol == icmp ip saddr 8.8.8.8         $(NFTcnt)  $(NFTset_1), testICMP32) 
  $(call ip4inRule111,chLocalIp4natForWard, ip saddr 114.114.114.114 udp dport 53 $(NFTcnt)  $(NFTset_1), testICMP31) 
  $(call ip4inRule111,chLocalIp4natForWard, ip saddr 8.8.8.8         udp dport 53 $(NFTcnt)  $(NFTset_1), testICMP32) 
)

### chLocalIp4nat : begin 

$(foreach aa1,$(NFTgroupCard_x_ALL),\
  $(foreach aa2,$(NFTgroupCard_x_$(aa1)),$(foreach aa3,$(NFTcardS_$(aa2)),\
    $(call ip4inRule111,chLocalIp4natForWard   , iif $(aa3) $(NFTcnt) accept) \
  ))\
)
$(ccall ip4inRule111,chLocalIp4natForWard  , $(NFTcnt) $(NFTset_1))
$(call ip4inRule111,chLocalIp4natForWard  , $(NFTcnt) accept)
### chLocalIp4nat : end


### _i1_N5FcheckSrcIp
$(foreach aa1,$(NFTgroupCard_x_ALL), $(foreach aa2,$(NFTgroupCard_y2_$(aa1)),\
$(call ip4inRuleAA1,_i1_N5FcheckSrcIp, ip saddr $(aa2) $(NFTcnt) goto chLocalIp4Card_$(aa1)_i3_N5FcheckDstIp) \
))

$(foreach aa1,lo, $(foreach aa2,$(NFTgroupCard_y2_lan) $(NFTgroupCard_y2_vpn),\
$(call ip4inRuleAA1,_i1_N5FcheckSrcIp, ip saddr $(aa2) ip daddr $(aa2) $(NFTcnt) accept) \
))

$(foreach aa1,$(NFTgroupCard_x_ALL), \
	$(if $(NFTdebug),$(call ip4inRuleAA1,_i1_N5FcheckSrcIp, $(NFTset_1),1) )\
	$(call ip4inRuleAA1,_i1_N5FcheckSrcIp, $(NFTcnt) drop) \
	)


### _i3_N5FcheckDstIp
$(foreach aa1,$(NFTgroupCard_x_ALL)  ,$(foreach aa2,$(NFTgroupCard_y5_$(aa1)),\
$(call ip4inRuleAA1,_i3_N5FcheckDstIp, ip daddr $(aa2) $(NFTcnt) goto chLocalIp4Card_$(aa1)_i5_N5FcheckProtocol_tcp) \
))

$(foreach aa1,lan  ,$(foreach aa2,$(NFTblackHoleIP),\
$(call ip4inRuleAA1,_i3_N5FcheckDstIp, ip daddr $(aa2) $(NFTcnt) drop) \
)\
$(call ip4inRuleAA1,_i3_N5FcheckDstIp, $(NFTcnt) goto chLocalIp4Card_$(aa1)_i5_N5FcheckProtocol_tcp) \
)

$(foreach aa1,$(NFTgroupCard_x_ALL), \
  $(call ip4inRuleAA1,_i3_N5FcheckDstIp, ip daddr 239.0.0.0/8 $(NFTcnt) drop #__$(aa1)__$(aa2)__8 )  \
  $(call ip4inRuleAA1,_i3_N5FcheckDstIp, ip daddr 224.0.0.0/8 $(NFTcnt) drop #__$(aa1)__$(aa2)__9 )  \
  $(call ip4inRuleAA1,_i3_N5FcheckDstIp, $(NFTset_1),1) \
  $(call ip4inRuleAA1,_i3_N5FcheckDstIp, $(NFTcnt) drop) \
)

# _i5_N5FcheckProtocol_tcp --> _i7_N5FcheckTcpDstPortNew , _j1_N5FcheckProtocol_udp 
$(foreach aa1,$(NFTgroupCard_x_ALL)  ,\
$(call ip4inRuleAA1,_i5_N5FcheckProtocol_tcp, ip protocol != tcp $(NFTcnt) goto chLocalIp4Card_$(aa1)_j1_N5FcheckProtocol_udp) \
$(call ip4inRuleAA1,_i5_N5FcheckProtocol_tcp, ct state { established } $(NFTcnt) accept) \
$(call ip4inRuleAA1,_i5_N5FcheckProtocol_tcp, ct state { related     } $(NFTcnt) accept) \
$(call ip4inRuleAA1,_i5_N5FcheckProtocol_tcp, ct state { new } $(NFTcnt) goto chLocalIp4Card_$(aa1)_i7_N5FcheckTcpDstPortNew) \
$(call ip4inRuleAA1,_i5_N5FcheckProtocol_tcp, $(NFTcnt) drop) \
)

# _i7_N5FcheckTcpDstPortNew
$(foreach aa1,$(NFTgroupCard_x_ALL)  ,\
  $(if $(filter lo,$(aa1)),\
    $(call ip4inRuleAA1,_i7_N5FcheckTcpDstPortNew, $(NFTcnt) accept) ,\
    $(foreach aa2,$(call sortUN, $(NFTtcpOUT_allow) $(NFTtcpIN_$(aa1))), \
      $(call ip4inRuleAA1,_i7_N5FcheckTcpDstPortNew, tcp dport $(aa2) $(NFTcnt) accept)  \
    )\
  )\
  $(if $(filter lan,$(aa1)), \
    $(foreach aa2,$(call sortUU, $(NFTgroupCard_y5_lan) ), \
      $(call ip4inRuleAA1,_i7_N5FcheckTcpDstPortNew, ip daddr $(aa2) $(NFTcnt) accept) ))\
  $(call ip4inRuleAA1,_i7_N5FcheckTcpDstPortNew, $(NFTset_1),1) \
  $(call ip4inRuleAA1,_i7_N5FcheckTcpDstPortNew, $(NFTcnt) drop) \
)

# _j1_N5FcheckProtocol_udp -> _j3_N5FcheckUdpDstPort , _j5_N5FcheckProtocol_icmp 
$(foreach aa1,$(NFTgroupCard_x_ALL)  ,\
$(call ip4inRuleAA1,_j1_N5FcheckProtocol_udp, ip protocol != udp $(NFTcnt) goto chLocalIp4Card_$(aa1)_j5_N5FcheckProtocol_icmp) \
$(call ip4inRuleAA1,_j1_N5FcheckProtocol_udp, ct state { established } $(NFTcnt) accept) \
$(call ip4inRuleAA1,_j1_N5FcheckProtocol_udp, ct state { related     } $(NFTcnt) accept) \
$(call ip4inRuleAA1,_j1_N5FcheckProtocol_udp, ct state { new } $(NFTcnt) goto chLocalIp4Card_$(aa1)_j3_N5FcheckUdpDstPort) \
$(call ip4inRuleAA1,_j1_N5FcheckProtocol_udp, $(NFTcnt) drop) \
)

# _j3_N5FcheckUdpDstPort
$(foreach aa1,lan,\
  $(call ip4inRuleAA1,_j3_N5FcheckUdpDstPort, ip daddr 239.0.0.0/8 $(NFTcnt) drop #__$(aa1)__$(aa2)__0 )  \
)
$(foreach aa1,$(NFTgroupCard_x_ALL),\
  $(foreach aa2,$(call sortUN,$(NFTudpOUT_allow) $(NFTudpIN_$(aa1)) ),\
    $(call ip4inRuleAA1,_j3_N5FcheckUdpDstPort, udp dport $(aa2) $(NFTcnt) accept #__$(aa1)__$(aa2)__1 )  \
  )\
)

$(foreach aa1,lo ,\
    $(call ip4inRuleAA1,_j3_N5FcheckUdpDstPort, $(NFTcnt) accept #__$(aa1)__$(aa2)__2 )  \
)

$(foreach aa1,$(NFTgroupCard_x_ALL)  ,\
  $(call ip4inRuleAA1,_j3_N5FcheckUdpDstPort, $(NFTset_1),1) \
  $(call ip4inRuleAA1,_j3_N5FcheckUdpDstPort, $(NFTcnt) drop) \
)

# _j5_N5FcheckProtocol_icmp -> _j7_N5FcheckProtocol_igmp 
$(foreach aa1,$(NFTgroupCard_x_ALL)  ,\
$(call ip4inRuleAA1,_j5_N5FcheckProtocol_icmp, ip protocol != icmp $(NFTcnt) goto chLocalIp4Card_$(aa1)_j7_N5FcheckProtocol_igmp) \
$(call ip4inRuleAA1,_j5_N5FcheckProtocol_icmp, icmp type { echo-reply   } $(NFTcnt) accept) \
$(call ip4inRuleAA1,_j5_N5FcheckProtocol_icmp, icmp type { echo-request } $(NFTcnt) accept) \
$(call ip4inRuleAA1,_j5_N5FcheckProtocol_icmp, icmp type { destination-unreachable } $(NFTcnt) drop) \
$(call ip4inRuleAA1,_j5_N5FcheckProtocol_icmp, $(NFTset_1),1) \
$(call ip4inRuleAA1,_j5_N5FcheckProtocol_icmp, $(NFTcnt) drop) \
)

# _j7_N5FcheckProtocol_igmp -> _j9_N5Funknown 
$(foreach aa1,$(NFTgroupCard_x_ALL)  ,\
$(call ip4inRuleAA1,_j7_N5FcheckProtocol_igmp, ip protocol != igmp $(NFTcnt) goto chLocalIp4Card_$(aa1)_j9_N5Funknown) \
$(call ip4inRuleAA1,_j7_N5FcheckProtocol_igmp, ip daddr 224.0.0.22 $(NFTcnt) drop) \
$(call ip4inRuleAA1,_j7_N5FcheckProtocol_igmp, $(NFTset_1),1) \
$(call ip4inRuleAA1,_j7_N5FcheckProtocol_igmp, $(NFTcnt) drop) \
)

# _j9_N5Funknown  ->
$(foreach aa1,$(NFTgroupCard_x_ALL)  ,\
$(call ip4inRuleAA1,_j9_N5Funknown, $(NFTset_1),1) \
$(call ip4inRuleAA1,_j9_N5Funknown, $(NFTcnt) accept) \
)

# NFTtext_29_ipv4_natForward end
endef
export NFTtext_29_ipv4_natForward:=$(NFTtext_29_ipv4_natForward)


